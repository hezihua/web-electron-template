{
  "name": "web-electron-template",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "electron:dev": "chcp 65001 && vite --config vite.config.electron.ts",
